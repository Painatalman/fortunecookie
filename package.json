{
  "name": "fortunecookie",
  "version": "0.1.0",
  "description": "Multilanguage fortune cookie teller",
  "main": "dist/main.js",
  "scripts": {
    "preinstall": "echo you are now installing default packages from the mc-nrfb generator",
    "postinstall": "echo you have just installed default packages for the mc-nrfb generator",
    "run-tests": "mocha --compilers js:babel-core/register,css:__tests__/__helpers__/ignore-styles.js --recursive ./__tests__",
    "test": "npm run run-tests -- --watch",
    "watch": "webpack -d --watch",
    "build": "webpack",
    "serve": "webpack-dev-server --inline",
    "start": "npm run watch || npm run serve",
    "send-to-gh-pages": "git subtree push --prefix dist origin gh-pages",
    "force-send-to-gh-pages": "git push origin `git subtree split --prefix dist master`:gh-pages --force",
    "build-and-send-to-gh-pages": "npm run build && npm run send-to-gh-pages",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "keywords": [
    "generator",
    "yeoman",
    "react",
    "node",
    "frontend"
  ],
  "author": "Carlos Batman <maiquedi@gmail.com>",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@kadira/storybook": "^2.35.3",
    "autoprefixer": "^6.7.7",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "chai": "^3.5.0",
    "css-loader": "^0.28.0",
    "enzyme": "^2.7.1",
    "eslint": "^3.9.1",
    "eslint-loader": "^1.7.1",
    "flow-bin": "^0.44.0",
    "jsdom": "^9.12.0",
    "mocha": "^3.2.0",
    "postcss-loader": "^1.3.3",
    "postcss-nested": "^1.0.1",
    "postcss-simple-vars": "^3.1.0",
    "react": "^15.5.4",
    "react-addons-test-utils": "^15.5.1",
    "react-dom": "^15.5.4",
    "react-router": "^3.0.5",
    "style-loader": "^0.16.1",
    "stylelint": "^7.10.1",
    "stylelint-loader": "6.2.0",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.2",
    "webpack-notifier": "^1.4.1"
  }
}
