{
  "env": {
    "browser": true
  },
  "globals": {
    "$": true
  },
  "rules": {
    "quotes": [1, "single"],
    "eol-last": 1,
    "prefer-template": 1,
    "comma-dangle": 1
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    }
  }
}
